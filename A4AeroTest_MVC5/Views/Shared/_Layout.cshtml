<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")


    <!--   wahid  --> <!--   wahid  --> <!--   wahid  -->

    <link href="~/Content/DataTables/datatables.min.css" rel="stylesheet" />


    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />
    <!--   datepicek  -->
    <link href="~/Content/themes/base/jquery-ui.min.css" rel="stylesheet" />

    <!--   datepicek  -->
    <link href="~/Content/sidebar/bootstrap-3/style5.css" rel="stylesheet" />
    @*<link href="~/Content/fontawesome-free/css/fontawesome.min.css" rel="stylesheet" />*@
    <style>

    </style>

    <!--   wahid  --> <!--   wahid  --> <!--   wahid  -->

</head>
<body>
    <nav class="navbar navbar-expand-sm bg-dark navbar-dark">
       
        <div class="collapse navbar-collapse" id="collapsibleNavbar">

            @Html.Partial("_LoginPartial")
        </div>
    </nav>



    <!--menu sidbar-->
    <div class="wrapper">
        <!-- Sidebar Holder -->
        <nav id="sidebar">
            <div class="sidebar-header">
                <img src="" alt="Logo" height="30" width="30" />
            </div>
            <ul class="list-unstyled components">
              
               
                <li>

                    <a href="#Businessmenu" data-toggle="collapse" aria-expanded="false">Business</a>
                    <ul class="collapse list-unstyled" id="Businessmenu">
                        <li>
                            @Html.ActionLink("Business", "Index", new { Controller = "Business" }, new { @class = "nav-link" })
                        </li>
                        <li>
                            @Html.ActionLink("List", "List", new { Controller = "Business" }, new { @class = "nav-link" })
                        </li>
                       
                    </ul>



                </li>
            </ul>

        </nav>
        <!-- Page Content Holder -->
        <div id="content" style="width:100%;">
            <nav class="navbar navbar-default">
                <div class="container-fluid">
                    <div class="navbar-header">
                        <button type="button" id="sidebarCollapse" class="navbar-btn">
                            <span></span>
                            <span></span>
                            <span></span>
                        </button>
                    </div>
                    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                        <ul class="nav navbar-nav navbar-right">
                            <li><a href="#">Page</a></li>
                            <li><a href="#">Page</a></li>
                            <li><a href="#">Page</a></li>
                            <li><a href="#">Page</a></li>
                        </ul>
                    </div>
                </div>
            </nav>

            <div class="body-content">
                @RenderBody()
                <hr />
                <footer>
                    <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
                </footer>
            </div>
        </div>
    </div>
    <!--menu sidbar-->

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)


    <!--   wahid  -->  <!--   wahid  --> <!--   wahid  -->
    <script src="~/Scripts/jquery-3.3.1.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>

    <script src="~/Content/DataTables/DataTables-1.10.18/js/jquery.dataTables.min.js"></script>
    <script src="~/Content/DataTables/DataTables-1.10.18/js/dataTables.bootstrap4.min.js"></script>
    <!--   datepicek  -->
    <script src="~/Scripts/jquery-ui-1.12.1.min.js"></script>
    <!--   datepicek  -->
    <!--   wahid  --> <!--   wahid  --> <!--   wahid  -->

    <script src="~/Content/commonJS/common.js"></script>
    <script type="text/javascript">
        var BaseUrl = "";
        var ip = location.host;
        if (ip == 'localhost:1216') {
            BaseUrl = 'http://localhost:1216/';
        } else {
            BaseUrl = 'http://' + ip + '/GreyPayrollNew/';
        }
        $(document).ready(function () {
            $('#sidebarCollapse').on('click', function () {
                $('#sidebar').toggleClass('active');
                $(this).toggleClass('active');
            });
        });

    </script>

</body>
</html>
